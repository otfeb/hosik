package beakjoon;

import java.util.Scanner;


public class bj_1110 {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		int n = scanner.nextInt();
		
		int ill = n%10;
		int ship = n%100/10;
		int sum = 0;
		
		while(true) {
			sum = ship+ill;
			String s = "ill"+sum%10;
			break;
		}
		System.out.println(s);
	}

}
