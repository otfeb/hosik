package day0619;

import java.util.Scanner;

public class KeyBoardIn_02 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		String name;
		int age;
		
		System.out.print("이름을 입력해 보세요: ");
		name = sc.nextLine();

		System.out.print("내이름은 "+name+"입니다");
		
	}

}
