package day0619;

import java.util.Scanner;

public class OperTest_07 {

	public static void main(String[] args) {

		Scanner scanner = new Scanner(System.in);
		
		int x,y;
		
		System.out.println("두수를 입력해 주세요");
		x = scanner.nextInt();
		y = scanner.nextInt();
		
		System.out.println("더하기= "+(x+y));

	}

}
