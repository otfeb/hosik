package day0616;

public class OperTest_07 {

	public static void main(String[] args) {

		// 기본자료형 8개
		// 1. 논리형 true, false
			boolean b = false;
			
		// 2. 문자형
			char ch = '가';
			// 기본적으로는 정수형
			
		// 3. 정수형
			byte a = -128; 	// 1byte -128 ~ 127
			short s = 32000; //16bit
			int c; 			// 32bit
			long num=100L; // 64bit

	}

}
