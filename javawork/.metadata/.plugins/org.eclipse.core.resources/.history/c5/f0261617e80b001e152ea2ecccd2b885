package day0616;

public class ParseArgs_04 {

	public static void main(String[] args) {

		String name = args[0];
		String su = args[1];
		String pay = args[2];
		String total = args[3];
		
		System.out.println("[상품정보]");
		System.out.println("상품명: " + name);
		System.out.println("수량: " + su);
		System.out.println("단가: " + pay);
		System.out.println("총금액: " + total);

	}

}
